{
  "name": "annotation-player",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "pages": "ng build --output-path docs --base-href annotation-player --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:elements": "ng build --configuration production --output-hashing none && npm run pack:elements",
    "pack:elements": "shx cat dist/annotation-player/{runtime,main}.js > dist/annotation-player/annotation-player.js",
    "build:components": "ng build --configuration production"
  },
  "dependencies": {
    "@angular/animations": "13.1.2",
    "@angular/cdk": "13.1.2",
    "@angular/common": "13.1.2",
    "@angular/compiler": "13.1.2",
    "@angular/core": "13.1.2",
    "@angular/elements": "13.1.2",
    "@angular/forms": "13.1.2",
    "@angular/platform-browser": "13.1.2",
    "@angular/platform-browser-dynamic": "13.1.2",
    "@angular/router": "13.1.2",
    "@datorama/akita": "^6.1.3",
    "@ngneat/effects-ng": "^1.0.0",
    "@ngneat/elf": "^1.0.2",
    "@ngneat/elf-devtools": "^1.1.0",
    "@ngneat/elf-entities": "^2.0.2",
    "@ngneat/elf-state-history": "^1.0.1",
    "@rx-angular/template": "^1.0.0-beta.29",
    "@types/offscreencanvas": "^2019.6.4",
    "immer": "^9.0.7",
    "rxjs": "^7.3.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.1.3",
    "@angular/cli": "13.1.3",
    "@angular/compiler-cli": "13.1.2",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "shx": "^0.3.3",
    "typescript": "~4.4.3"
  }
}
